fig,axes=plt.subplots(2,3,figsize=(15,10))
sns.boxplot(x="sugar",y="blood glucose random",data=data1_new,ax=axes[0,1])
sns.boxplot(x="sugar",y="age",data=data1_new,ax=axes[0,0])

sns.boxplot(x="sugar",y="blood urea",data=data1_new,ax=axes[0,2])
sns.boxplot(x="sugar",y="serum creatinine",data=data1_new,ax=axes[1,0])
sns.boxplot(x="sugar",y="sodium",data=data1_new,ax=axes[1,1])
sns.boxplot(x="sugar",y="sodium",data=data1_new,ax=axes[1,2])
